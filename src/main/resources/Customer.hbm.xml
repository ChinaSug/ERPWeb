<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.Customer" table="T_CUSTOMER">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="name" column="NAME" type="string"/>
	
		<property name="contactPerson" column="CONTACT_PERSON" type="string"/>
		<property name="mobileNum" column="MOBILE_NUM" type="string"/>
		<property name="email" column="EMAIL" type="string"/>
		<property name="status" column="STATUS" type="string"/>
		<property name="createBy" column="CREATE_BY" type="string"/>
		<property name="createDate" column="CREATE_DATE" type="timestamp"/>
		
		
	</class>
</hibernate-mapping>
