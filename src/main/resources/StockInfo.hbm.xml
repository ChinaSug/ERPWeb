<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.StockInfo" table="T_STOCK_INFO" lazy="false">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="stockNum" column="STOCK_NUM" type="string"/>
		<property name="stockTypeOid" column="STOCK_TYPE_OID" type="long"/>
		<property name="warehouseOid" column="WAREHOUSE_OID" type="long"/>
		<property name="stockPerson" column="STOCK_PERSON" type="string"/>
		<property name="stockDate" column="STOCK_DATE" type="date"/>
		<property name="itemOid" column="ITEM_OID" type="long"/>
		<property name="stockAmt" column="STOCK_AMT" type="double"/>
		<property name="unitPrice" column="UNIT_PRICE" type="double"/>
		<property name="totalPrice" column="TOTAL_PRICE" type="double"/>
		<property name="customerOid" column="CUSTOMER_OID" type="long"/>
		<property name="remark" column="REMARK" type="string"/>
		<property name="status" column="STATUS" type="string"/>
		<property name="createBy" column="CREATE_BY" type="string"/>
		<property name="createDate" column="CREATE_DATE" type="timestamp"/>
		<property name="lastModifyBy" column="LAST_MODIFY_BY" type="string"/>
		<property name="lastModifyDate" column="LAST_MODIFY_DATE" type="timestamp"/>
		
		<property name="stockItemType" column="STOCK_ITEM_TYPE" type="string"/>
		<property name="bomDetailOid" column="BOM_DETAIL_OID" type="long"/>
		
	</class>
</hibernate-mapping>   
