<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.Machine" table="T_MACHINE" lazy="false">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="name" column="NAME" type="string"/>
		<property name="brand" column="BRAND" type="string"/>
		<property name="type" column="TYPE" type="string"/>
		<property name="tieBar" column="TIE_BAR" type="string"/>
		<property name="fpSize" column="FP_SIZE" type="string"/>
		<property name="openQuan" column="OPEN_QUAN" type="string"/>
		<property name="thick" column="THICK" type="string"/>
		<property name="screwDia" column="SCREW_DIA" type="string"/>
		<property name="maxInjPrs" column="MAX_INJ_PRS" type="string"/>
		<property name="injWgt" column="INJ_WGT" type="string"/>
		<property name="remark" column="REMARK" type="string"/>
		
	</class>
</hibernate-mapping>   
