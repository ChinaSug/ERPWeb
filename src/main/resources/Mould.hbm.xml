<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.Mould" table="T_MOULD" lazy="false">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="name" column="NAME" type="string"/>
		<property name="customerOid" column="CUSTOMER_OID" type="long"/>
		<property name="scale" column="SCALE" type="string"/>
		<property name="hole" column="HOLE" type="string"/>
		<property name="made" column="MADE" type="string"/>
		<property name="manu" column="MANU" type="string"/>
		<property name="date" column="DATE" type="string"/>
		<property name="outDate" column="OUT_DATE" type="string"/>
		<property name="place" column="PLACE" type="string"/>
		<property name="remark" column="REMARK" type="string"/>
	</class>
</hibernate-mapping>   
