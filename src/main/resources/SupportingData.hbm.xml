<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.SupportingData" table="T_SUPPORT_DATA" lazy="false">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="dataCode" column="DATA_CODE" type="string"/>
		<property name="dataName" column="DATA_NAME" type="string"/>
		<property name="dataType" column="DATA_TYPE" type="string"/>
	</class>
</hibernate-mapping>   
