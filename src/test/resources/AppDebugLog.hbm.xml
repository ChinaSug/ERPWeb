<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.AppDebugLog" table="T_APP_DEBUG_LOG" lazy="false">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="userId" column="USER_ID" type="string"/>
		<property name="function" column="FUNC" type="string"/>
		<property name="input" column="INPUT" type="string"/>
		<property name="output" column="OUTPUT" type="string"/>
		<property name="createDate" column="CREATE_DATE" type="timestamp"/>
	</class>
</hibernate-mapping>   
