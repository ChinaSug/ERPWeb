<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.UserPriv" table="T_USER_PRIV" lazy="false">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="userOid" column="USER_OID" type="long"/>
		<property name="menuOid" column="MENU_OID" type="long"/>
		<property name="seq" column="SEQ" type="integer"/>
		
	</class>
</hibernate-mapping>   
