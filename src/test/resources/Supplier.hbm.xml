<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.Supplier" table="T_SUPPLIER" lazy="false">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="supplierId" column="SUPP_NUM" type="string"/>
		<property name="supplierName" column="SUPP_NAME" type="string"/>
		<property name="type" column="SUPP_TYPE" type="string"/>
		<property name="prodName" column="PROD_NAME" type="string"/>
		<property name="contactPerson" column="CONTACT_PERSON" type="string"/>
		<property name="contactPhone" column="MOBILE_NUM" type="string"/>
		<property name="email" column="EMAIL" type="string"/>
		<property name="status" column="STATUS" type="string"/>
		<property name="createBy" column="CREATE_BY" type="string"/>
		<property name="createDate" column="CREATE_DATE" type="timestamp"/>
		<property name="lastModifyBy" column="LAST_MODIFY_BY" type="string"/>
		<property name="lastModifyDate" column="LAST_MODIFY_DATE" type="timestamp"/>
		
	</class>
</hibernate-mapping>   
