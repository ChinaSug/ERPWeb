<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.abs.ps.domain.User" table="T_USER" lazy="false">
		<id name="oid" column="OID" type="long">
			<generator class="identity"/>
		</id>
		<property name="userId" column="USER_ID" type="string"/>
		<property name="password" column="PASSWORD" type="string"/>
		<property name="userName" column="USER_NAME" type="string"/>
		<property name="status" column="STATUS" type="string"/>
		<property name="orgOid" column="ORG_OID" type="long"/>
		<property name="isAdmin" column="IS_ADMIN" type="boolean"/>
		<property name="email" column="EMAIL" type="string"/>
		<property name="mobileNum" column="MOBILE_NUM" type="string"/>
		<property name="sex" column="SEX" type="string"/>
		<property name="imgUrl" column="IMG_URL" type="string"/>
		<property name="lastLoginTime" column="LAST_LOGIN_TIME" type="timestamp"/>
		<property name="createBy" column="CREATE_BY" type="string"/>
		<property name="createDate" column="CREATE_DATE" type="timestamp"/>
		<property name="lastModifyBy" column="LAST_MODIFY_BY" type="string"/>
		<property name="lastModifyDate" column="LAST_MODIFY_DATE" type="timestamp"/>
		
	</class>
</hibernate-mapping>   
